<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:background="@drawable/mainhomepage"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/mainName"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/circle"
        android:layout_gravity="center"
        android:layout_marginTop="35dp"
        android:fontFamily="@font/nanumb"
        android:text="원준"
        android:gravity="center"
        android:textColor="#ffffff"
        android:textSize="30dp"/>
    <TextView
        android:id="@+id/textName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="좋은 꿈 꾸세요. 신원준 님"
        android:gravity="center"
        android:layout_marginTop="10dp"
        android:textColor="#ffffff"
        android:fontFamily="@font/nanumb"
        android:textSize="16dp"/>
    <LinearLayout
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:orientation="horizontal">
        <LinearLayout
            android:layout_width="90dp"
            android:layout_height="60dp"
            android:background="@drawable/square"
            android:orientation="vertical">
            <TextView
                android:layout_margin="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="12dp"
                android:fontFamily="@font/nanumb"
                android:text="수면\n시작시간"
                android:textColor="#000000"/>
            <TextView
                android:id="@+id/sleepStartTime"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="23시 21분"
                android:textSize="12dp"
                android:textColor="#000000"
                android:layout_marginLeft="5dp"
                android:fontFamily="@font/nanumb"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="90dp"
            android:layout_height="60dp"
            android:layout_marginLeft="22dp"
            android:background="@drawable/square"
            android:orientation="vertical">
            <TextView
                android:layout_margin="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="12dp"
                android:fontFamily="@font/nanumb"
                android:text="수면\n종료시간"
                android:textColor="#000000"/>
            <TextView
                android:id="@+id/sleepEndTime"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="08시 21분"
                android:textSize="12dp"
                android:textColor="#000000"
                android:layout_marginLeft="5dp"
                android:fontFamily="@font/nanumb"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="90dp"
            android:layout_height="60dp"
            android:layout_marginLeft="22dp"
            android:background="@drawable/square"
            android:orientation="vertical">
            <TextView
                android:layout_margin="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="12dp"
                android:fontFamily="@font/nanumb"
                android:text="총\n수면시간"
                android:textColor="#000000"/>
            <TextView
                android:id="@+id/sleepAlltime"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="9시간 21분"
                android:textSize="12dp"
                android:textColor="#000000"
                android:layout_marginLeft="5dp"
                android:fontFamily="@font/nanumb"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="240dp"
        android:layout_margin="20dp"
        android:background="@drawable/square_big"
        android:orientation="vertical">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="14dp"
            android:textColor="#000000"
            android:text="최근 수면 그래프"
            android:gravity="center"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/nanumb"/>
        <View
            android:id="@+id/graph"
            android:layout_width="match_parent"
            android:layout_height="90dp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"/>
        <LinearLayout
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:background="@drawable/circle_purple"
                android:gravity="center"
                android:orientation="vertical">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="깊은잠"
                    android:textSize="10dp"
                    android:layout_gravity="center"
                    android:textColor="#000000"
                    android:gravity="center"/>
                <TextView
                    android:id="@+id/deep_sleep"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    android:textSize="10dp"
                    android:gravity="center"
                    android:layout_gravity="center"
                    android:text="2187"/>
            </LinearLayout>
            <LinearLayout
                android:layout_marginLeft="42dp"
                android:layout_marginRight="43dp"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:gravity="center"
                android:orientation="vertical">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="수면시간"
                    android:textSize="10dp"
                    android:layout_gravity="center"
                    android:textColor="#000000"
                    android:gravity="center"/>
                <TextView
                    android:id="@+id/sleep_time"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    android:textSize="10dp"
                    android:gravity="center"
                    android:layout_gravity="center"
                    android:text="3333"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:gravity="center"
                android:background="@drawable/circle_pink"
                android:orientation="vertical">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="얇은 잠"
                    android:textSize="10dp"
                    android:layout_gravity="center"
                    android:textColor="#000000"
                    android:gravity="center"/>
                <TextView
                    android:id="@+id/rem_sleep"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    android:textSize="10dp"
                    android:gravity="center"
                    android:layout_gravity="center"
                    android:text="2918"/>
            </LinearLayout>
        </LinearLayout>
        <TextView
            android:id="@+id/sleep_Jumsu"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="당신의 수면 점수는 76점 입니다."
            android:textSize="18dp"
            android:fontFamily="@font/nanumb"
            android:textColor="#000000"
            android:layout_marginTop="18dp"
            android:gravity="center"/>
    </LinearLayout>
    <ListView
        android:id="@+id/list_alarm"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="20dp"/>

</LinearLayout>